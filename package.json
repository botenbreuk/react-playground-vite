{
  "name": "react-playground-vite",
  "private": true,
  "version": "0.0.1",
  "dependencies": {
    "@42.nl/jarb-final-form": "3.1.4",
    "bootstrap": "5.3.2",
    "bootstrap-icons": "1.11.1",
    "classnames": "2.3.2",
    "final-form": "4.20.10",
    "final-form-arrays": "3.1.0",
    "immutability-helper": "3.1.1",
    "lodash": "4.17.21",
    "moment": "2.29.4",
    "react": "18.2.0",
    "react-dnd": "16.0.1",
    "react-dnd-html5-backend": "16.0.1",
    "react-dom": "18.2.0",
    "react-final-form": "6.5.9",
    "react-final-form-arrays": "3.1.4",
    "react-icons": "4.11.0",
    "react-router-dom": "6.16.0",
    "reactstrap": "9.2.0",
    "zod": "3.22.2"
  },
  "devDependencies": {
    "@types/lodash": "4.14.199",
    "@types/node": "20.7.2",
    "@types/react": "18.2.23",
    "@types/react-dom": "18.2.8",
    "@types/react-router-dom": "5.3.3",
    "@typescript-eslint/eslint-plugin": "6.7.3",
    "@typescript-eslint/parser": "6.7.3",
    "@vitejs/plugin-react-swc": "3.4.0",
    "better-npm-audit": "3.7.3",
    "eslint": "8.50.0",
    "eslint-config-react-app": "7.0.1",
    "eslint-plugin-flowtype": "8.0.3",
    "eslint-plugin-import": "2.28.1",
    "eslint-plugin-jsx-a11y": "6.7.1",
    "eslint-plugin-react": "7.33.2",
    "husky": "8.0.3",
    "lint-staged": "14.0.1",
    "prettier": "3.0.3",
    "prettier-plugin-organize-imports": "3.2.3",
    "sass": "1.68.0",
    "stylelint": "15.10.3",
    "stylelint-config-prettier-scss": "1.0.0",
    "stylelint-config-standard-scss": "11.0.0",
    "terser": "5.20.0",
    "typescript": "5.2.2",
    "vite": "4.4.9",
    "vite-plugin-checker": "0.6.2"
  },
  "scripts": {
    "better-audit": "node node_modules/better-npm-audit audit",
    "build": "npm run create-env && tsc && vite build",
    "create": "npm run create-env",
    "create-env": "echo \"SKIP_PREFLIGHT_CHECK=true\nVITE_VERSION=${npm_package_version}\nBUILD_TIME=\"$(date +%Y-%m-%dT%H:%M:%S%z)\"\n\" > .env",
    "lint": "eslint src --ext js,jsx,ts,tsx --max-warnings=0",
    "preview": "vite build --mode preview && vite preview",
    "start": "npm run create && vite",
    "start-api": "cd rest-api && npm start",
    "stylelint": "stylelint src/**/*.{css,scss}",
    "stylelint-check": "stylelint-config-prettier-scss-check",
    "ts": "tsc --version && tsc"
  },
  "lint-staged": {
    "{src,test}/**/*.{js,jsx,json,scss,tsx,ts}": [
      "prettier --single-quote --write"
    ]
  }
}
