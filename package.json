{
  "name": "react-playground-vite",
  "private": true,
  "version": "0.0.1",
  "dependencies": {
    "@42.nl/jarb-final-form": "3.1.4",
    "bootstrap": "5.3.2",
    "bootstrap-icons": "1.11.3",
    "check-dependencies": "2.0.0",
    "classnames": "2.5.1",
    "final-form": "4.20.10",
    "final-form-arrays": "3.1.0",
    "immutability-helper": "3.1.1",
    "lodash": "4.17.21",
    "moment": "2.30.1",
    "react": "18.2.0",
    "react-dnd": "16.0.1",
    "react-dnd-html5-backend": "16.0.1",
    "react-dom": "18.2.0",
    "react-final-form": "6.5.9",
    "react-final-form-arrays": "3.1.4",
    "react-icons": "5.0.1",
    "react-router-dom": "6.21.2",
    "reactstrap": "9.2.1",
    "zod": "3.22.4"
  },
  "devDependencies": {
    "@types/express": "4.17.21",
    "@types/lodash": "4.14.202",
    "@types/node": "20.11.0",
    "@types/react": "18.2.47",
    "@types/react-dom": "18.2.18",
    "@types/react-router-dom": "5.3.3",
    "@typescript-eslint/eslint-plugin": "6.18.1",
    "@typescript-eslint/parser": "6.18.1",
    "@vitejs/plugin-react-swc": "3.5.0",
    "better-npm-audit": "3.7.3",
    "eslint": "8.56.0",
    "eslint-config-react-app": "7.0.1",
    "eslint-plugin-flowtype": "8.0.3",
    "eslint-plugin-import": "2.29.1",
    "eslint-plugin-jsx-a11y": "6.8.0",
    "eslint-plugin-react": "7.33.2",
    "husky": "8.0.3",
    "lint-staged": "15.2.0",
    "prettier": "3.1.1",
    "prettier-plugin-organize-imports": "3.2.4",
    "sass": "1.69.7",
    "stylelint": "16.1.0",
    "stylelint-config-prettier-scss": "1.0.0",
    "stylelint-config-standard-scss": "13.0.0",
    "terser": "5.26.0",
    "typescript": "5.3.3",
    "vite": "5.0.11",
    "vite-plugin-checker": "0.6.2"
  },
  "scripts": {
    "better-audit": "node node_modules/better-npm-audit audit --production",
    "build": "tsc && vite build",
    "check-dependencies": "check-dependencies",
    "check-project": "npm run check-dependencies && npm run lint && npm run ts",
    "prepare": "husky install",
    "lint": "eslint src --ext js,jsx,ts,tsx --max-warnings=0",
    "prettier": "prettier --write ./src",
    "preview": "vite build --mode preview && vite preview",
    "start": "vite",
    "start-api": "cd rest-api && npm start",
    "stylelint": "stylelint src/**/*.{css,scss}",
    "stylelint-check": "stylelint-config-prettier-scss-check",
    "ts": "tsc --version && tsc"
  },
  "lint-staged": {
    "{src,test}/**/*.{js,jsx,json,scss,tsx,ts}": [
      "prettier --single-quote --write"
    ]
  }
}
