  "version": "0.0.1",
  "dependencies": {
    "@42.nl/jarb-final-form": "3.1.5",
    "@42.nl/react-url": "3.3.1",
    "@hookform/resolvers": "5.2.2",
    "@tanstack/react-query": "5.90.2",
    "axios": "1.12.2",
    "bootstrap": "5.3.8",
    "bootstrap-icons": "1.13.1",
    "clsx": "2.1.1",
    "date-fns": "4.1.0",
    "final-form": "5.0.0",
    "final-form-arrays": "4.0.0",
    "immutability-helper": "3.1.1",
    "lodash": "4.17.21",
    "moment": "2.30.1",
    "react": "19.2.0",
    "react-datepicker": "8.7.0",
    "react-datetime": "3.3.1",
    "react-dnd": "16.0.1",
    "react-dnd-html5-backend": "16.0.1",
    "react-dom": "19.2.0",
    "react-final-form": "7.0.0",
    "react-final-form-arrays": "4.0.0",
    "react-hook-form": "7.65.0",
    "react-icons": "5.5.0",
    "react-router": "7.9.4",
    "reactstrap": "9.2.3",
    "zod": "4.1.12"
  },
  "devDependencies": {
    "@types/express": "5.0.3",
    "@types/lodash": "4.17.20",
    "@types/node": "24.7.2",
    "@types/react": "19.2.2",
    "@types/react-dom": "19.2.1",
    "@vitejs/plugin-react-swc": "4.1.0",
    "better-npm-audit": "3.11.0",
    "csp-header": "6.2.0",
    "eslint": "9.37.0",
    "eslint-config-prettier": "10.1.8",
    "eslint-plugin-check-file": "3.3.0",
    "eslint-plugin-import": "2.32.0",
    "eslint-plugin-react": "7.37.5",
    "eslint-plugin-react-hooks": "7.0.0",
    "husky": "9.1.7",
    "lint-staged": "16.2.4",
    "prettier": "3.6.2",
    "prettier-plugin-organize-imports": "4.3.0",
    "sass": "1.93.2",
    "stylelint": "16.25.0",
    "stylelint-config-prettier-scss": "1.0.0",
    "stylelint-config-standard-scss": "16.0.0",
    "terser": "5.44.0",
    "typescript": "5.9.3",
    "typescript-eslint": "8.46.0",
    "vite": "7.1.9",
    "vite-plugin-checker": "0.11.0"
  },
  "scripts": {
    "better-audit": "node node_modules/better-npm-audit audit --production",
    "build": "tsc && vite build",
    "check-dependencies": "node ci/check-dependencies.ts",
    "check-project": "npm run check-dependencies && npm run lint && npm run ts",
    "lint": "TIMING=all ESLINT_USE_FLAT_CONFIG=true eslint src --max-warnings 0",
    "prepare": "husky",
    "prettier": "prettier --write ./src",
    "preview": "vite build --mode preview && vite preview",
    "start": "vite",
    "start-api": "cd _rest-api && npm start",
    "stylelint": "stylelint src/**/*.{css,scss}",
    "stylelint-check": "stylelint-config-prettier-scss-check",
    "test-all": "npm run check-dependencies && npm run lint && npm run ts",
    "ts": "tsc --version && tsc"
  },
  "overrides": {
    "@42.nl/final-form-field-validation": {
      "react": "19.1.0",
      "final-form": "5.0.0",
      "react-final-form": "7.0.0"
    },
    "@42.nl/jarb-final-form": {
      "react-final-form": "7.0.0"
    }
  },
  "lint-staged": {
    "{src,test}/**/*.{js,jsx,json,scss,tsx,ts}": [
      "prettier --single-quote --write"
    ]
  }
}
