    "@42.nl/react-url": "3.3.0",
    "@tanstack/react-query": "5.66.0",
    "axios": "1.7.9",
    "bootstrap": "5.3.3",
    "bootstrap-icons": "1.11.3",
    "check-dependencies": "2.0.0",
    "classnames": "2.5.1",
    "date-fns": "4.1.0",
    "final-form": "4.20.10",
    "final-form-arrays": "3.1.0",
    "immutability-helper": "3.1.1",
    "lodash": "4.17.21",
    "moment": "2.30.1",
    "react": "19.0.0",
    "react-datepicker": "8.0.0",
    "react-datetime": "3.3.1",
    "react-dnd": "16.0.1",
    "react-dnd-html5-backend": "16.0.1",
    "react-dom": "19.0.0",
    "react-final-form": "6.5.9",
    "react-final-form-arrays": "3.1.4",
    "react-icons": "5.4.0",
    "react-router": "7.0.2",
    "reactstrap": "9.2.3",
    "zod": "3.24.2"
  },
  "devDependencies": {
    "@types/express": "5.0.0",
    "@types/lodash": "4.17.15",
    "@types/node": "22.13.1",
    "@types/react": "19.0.8",
    "@types/react-dom": "19.0.3",
    "@typescript-eslint/eslint-plugin": "8.24.0",
    "@typescript-eslint/parser": "8.24.0",
    "@vitejs/plugin-react-swc": "3.8.0",
    "better-npm-audit": "3.11.0",
    "csp-header": "6.0.0",
    "eslint": "8.57.1",
    "eslint-config-prettier": "10.0.1",
    "eslint-config-react-app": "7.0.1",
    "eslint-plugin-check-file": "2.8.0",
    "eslint-plugin-flowtype": "8.0.3",
    "eslint-plugin-import": "2.31.0",
    "eslint-plugin-jsx-a11y": "6.10.2",
    "eslint-plugin-react": "7.37.4",
    "eslint-plugin-react-hooks": "5.1.0",
    "husky": "9.1.7",
    "lint-staged": "15.4.3",
    "prettier": "3.5.0",
    "prettier-plugin-organize-imports": "4.1.0",
    "sass": "1.84.0",
    "stylelint": "16.14.1",
    "stylelint-config-prettier-scss": "1.0.0",
    "stylelint-config-standard-scss": "14.0.0",
    "terser": "5.38.2",
    "typescript": "5.7.3",
    "vite": "6.1.0",
    "vite-plugin-checker": "0.8.0"
  },
  "scripts": {
    "better-audit": "node node_modules/better-npm-audit audit --production",
    "build": "tsc && vite build",
    "check-dependencies": "check-dependencies",
    "check-project": "npm run check-dependencies && npm run lint && npm run ts",
    "prepare": "husky",
    "lint": "eslint src --ext js,jsx,ts,tsx --max-warnings=0",
    "prettier": "prettier --write ./src",
    "preview": "vite build --mode preview && vite preview",
    "start": "vite",
    "start-api": "cd _rest-api && npm start",
    "stylelint": "stylelint src/**/*.{css,scss}",
    "stylelint-check": "stylelint-config-prettier-scss-check",
    "test-all": "npm run check-dependencies && npm run lint && npm run ts",
    "ts": "tsc --version && tsc"
  },
  "overrides": {
    "@42.nl/final-form-field-validation": {
      "react": "19.0.0"
    },
    "react-datepicker": {
      "react": "19.0.0",
      "react-dom": "19.0.0"
    },
    "react-final-form": {
      "react": "19.0.0"
    },
    "react-final-form-arrays": {
      "react": "19.0.0"
    }
  },
  "lint-staged": {
    "{src,test}/**/*.{js,jsx,json,scss,tsx,ts}": [
      "prettier --single-quote --write"
    ]
  }
}
